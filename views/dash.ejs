<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>SB Admin 2 - Bootstrap Admin Theme</title>

    <% include importCss %>

</head>

<body>

    <div id="wrapper">

        <% include dashNavs %>

        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Dashboard</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-bar-chart-o fa-fw"></i> Datos
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                          <div class="list-group">
                              <a href="#" class="list-group-item">
                                  <i class="fa fa-comment fa-fw"></i> Total Tweets
                                  <span class="pull-right text-muted small"><em><%= Ttweets %></em>
                                  </span>
                              </a>
                              <a href="#" class="list-group-item">
                                  <i class="fa fa-twitter fa-fw"></i> Total Usuarios
                                  <span class="pull-right text-muted small"><em><%= Tusuaios %></em>
                                  </span>
                              </a>
                              <a href="#" class="list-group-item">
                                  <i class="fa fa-envelope fa-fw"></i> Total Imagenes
                                  <span class="pull-right text-muted small"><em><%= Timagenes %></em>
                                  </span>
                              </a>
                              <a href="#" class="list-group-item">
                                  <i class="fa fa-tasks fa-fw"></i> Total Categorias
                                  <span class="pull-right text-muted small"><em>43 minutes ago</em>
                                  </span>
                              </a>
                          </div>
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-8 -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

    <% include importJs %>

</body>

</html>
